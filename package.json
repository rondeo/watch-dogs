    "@angular/forms": "^5.1.1",
    "@angular/http": "^5.1.1",
    "@angular/material": "^5.0.1",
    "@angular/platform-browser": "^5.1.1",
    "@angular/platform-browser-dynamic": "^5.1.1",
    "@angular/router": "^5.1.1",
    "autobahn": "^17.5.2",
    "core-js": "^2.5.3",
    "crypto-js": "^3.1.9-1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "moment": "^2.19.4",
    "node-bittrex-api": "^0.8.1",
    "nodemon": "^1.13.3",

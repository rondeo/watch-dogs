  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "ng serve --port 4201 --proxy-config proxy.conf.json --live-reload false",
    "live-reload": "ng serve --port 4201 --proxy-config proxy.conf.json --live-reload true",
    "verify": "ng eject --target=production",
    "prod": "ng build --prod",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "es6": "./node_modules/.bin/http-server -c-1 .",
    "watch": "./node_modules/.bin/nodemon  -w . --ext \".ts\" --exec \"npm run start\"",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
